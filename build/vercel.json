{
  "version": 2,
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/landing/(.*)",
      "destination": "/landing/$1"
    },
    {
      "source": "/(favicon.ico|logo192.png|logo512.png|manifest.json|robots.txt|sample.pdf)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|pdf))",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
